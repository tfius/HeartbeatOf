                 tmod3 = toString(((x+1)*(y+1)*shiftt)%360);
                  heart = string(abi.encodePacked(heart, '<g fill="',color,'" class="bs" transform="translate(', toString(500+x*750),',', toString(500+y*750),') rotate(',tmod3,')">', heart2,'</g>'));
            }
         }
        heart = string(abi.encodePacked(heart, '</g>'));
        
        heart = string(abi.encodePacked(heart, '<text x="50%" y="375" dominant-baseline="middle" text-anchor="middle" style="direction:rtl;font-size:28px;font-family:Gamja Flower">',
                                        tokenName[tokenId],
                                        '</text>'));


        return string(
            abi.encodePacked(
                '<svg xmlns="http://www.w3.org/2000/svg" preserveAspectRatio="xMinYMin meet" viewBox="0 0 400 400"><defs><style type="text/css">@import url("https://fonts.googleapis.com/css?family=Indie+Flower|Gamja+Flower");</style></defs><style>.b {fill:red;} .bo {mix-blend-mode:overlay} .bs {mix-blend-mode:hard-light;stroke-dasharray:5;stroke:#90000011; stroke-width:121;} .bl {mix-blend-mode:color-dodge;stroke-dasharray:6;stroke:#ffffff05;stroke-width:200;}</style><rect width="100%" height="100%" fill="orange"/>',
                '<g class="b" transform="scale(1) rotate(-13', 
